{
    "description": "This repository contains Aspose Storage Cloud SDK for PHP source code. Aspose Storage Cloud SDK for PHP is a true REST API that provides managers for multiple storage types.",
    "name": "aspose/storage-sdk-php",
    "version": "18.4",
    "license": "MIT",
    "type": "library",
    "keywords" : [
        "aspose",
        "php",
        "aspose storage cloud"
	],
    "homepage": "https://github.com/aspose-storage-cloud/aspose-storage-cloud-php",
    "authors": [
        {
            "name": "Aspose",
            "homepage": "https://github.com/aspose-storage-cloud"
        }
    ],
    "config": {
        "platform": {
        "php": "5.6.32"
        }
    },
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-0": {
            "Aspose": "src/",
            "BaseTest": "tests/"
        }
    }
}
